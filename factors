import math

def factorize(n):
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            return str(n) + "=" + str(i) + "*" + str(n//i)
    return str(n) + " is prime"

with open("input.txt", "r") as f:
    for line in f:
        n = int(line.rstrip())
        print(factorize(n))
